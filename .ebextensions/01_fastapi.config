option_settings:
  aws:elasticbeanstalk:application:environment:
    PYTHONPATH: "/var/app/current:$PYTHONPATH"
  aws:elasticbeanstalk:container:python:
    WSGIPath: "server:app"

container_commands:
  01_initdb:
    command: "source /var/app/venv/*/bin/activate && python3 database.py"
    leader_only: true